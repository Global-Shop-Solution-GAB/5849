Program.Sub.LoadDictionary.Start
	F.Intrinsic.Control.Try
	V.Local.sError.Declare
	
		V.Local.iCnt.Declare
		V.Local.sDicName.Declare
		V.Local.sDicValues.Declare
		V.Local.sDicName.Set(V.Args.sDictionaryName)
		V.Local.sDicValues.Set(V.Args.sDictionaryValues)
		F.Data.Dictionary.Create(V.Local.sDicName)
		F.Intrinsic.String.Split(V.Local.sDicValues,"*!*",V.Local.sDicValues)
		F.Intrinsic.Control.For(V.Local.iCnt,0,V.Local.sDicValues.UBound)
			F.Data.Dictionary.AddItem(V.Local.sDicName,V.Local.iCnt,V.Local.sDicValues(V.Local.iCnt))
		F.Intrinsic.Control.Next(V.Local.iCnt)
		F.Intrinsic.Variable.AddRV("sDictionaryName",V.Local.sDicName)
		
	F.Intrinsic.Control.Catch
		F.Intrinsic.Control.CallSub(Catching,"Sub",V.Ambient.CurrentSubroutine,"ErrorNo",V.Ambient.ErrorNumber,"ErrorDesc",V.Ambient.ErrorDescription) 
	F.Intrinsic.Control.EndTry
Program.Sub.LoadDictionary.End

Program.Sub.frmDeleteConfirm_SelectionMade.Start
	F.Intrinsic.Control.Try
	V.Local.sError.Declare
	
		V.Local.sSQL.Declare
		F.Intrinsic.Control.If(V.Args.ResultString,=,"YES")
			F.Intrinsic.String.Build("delete from GCG_{2}_{1} where MACHINEID='{0}'",V.Screen.frmDeleteConfirm!lblMachineID.Caption,V.Screen.frmDeleteConfirm!lblForm.Caption,V.Global.iARCID,V.Local.sSQL)
			F.ODBC.Connection!con.Execute(V.Local.sSQL)
			F.Intrinsic.Control.If(V.Screen.frmDeleteConfirm!lblForm.Caption,=,"TREE_SHAKER")
				'Refresh grid data...
				F.Intrinsic.Control.CallSub(LoadData_Shakers)
			F.Intrinsic.Control.Else
				'Delete MACHINE_EXTRA record, too...
				F.Intrinsic.String.Build("delete from GCG_{1}_MACHINE_EXTRA where MACHINEID='{0}'",V.Screen.frmDeleteConfirm!lblMachineID.Caption,V.Global.iARCID,V.Local.sSQL)
				F.ODBC.Connection!con.Execute(V.Local.sSQL)
				'Refresh grid data...
				F.Intrinsic.Control.CallSub(LoadData_WindMachines)
			F.Intrinsic.Control.EndIf
			Gui.frmDeleteConfirm.lblMachineID.Caption("")
			Gui.frmDeleteConfirm.lblForm.Caption("")
		F.Intrinsic.Control.EndIf
	
	F.Intrinsic.Control.Catch
		F.Intrinsic.Control.CallSub(Catching,"Sub",V.Ambient.CurrentSubroutine,"ErrorNo",V.Ambient.ErrorNumber,"ErrorDesc",V.Ambient.ErrorDescription) 
	F.Intrinsic.Control.EndTry
Program.Sub.frmDeleteConfirm_SelectionMade.End

Program.Sub.CheckDatabase.Start
	F.Intrinsic.Control.Try
	V.Local.sError.Declare
	
		V.Local.bTableExists1.Declare
		V.Local.bTableExists2.Declare
		V.Local.bTableExists3.Declare
		
		F.Intrinsic.UI.InvokeWaitDialog("Checking the database...")
		
		F.ODBC.Connection!con.TableExists("GCG_5849_TREE_SHAKER",V.Local.bTableExists1)
		F.ODBC.Connection!con.TableExists("GCG_5849_WIND_MACHINE",V.Local.bTableExists2)
		F.ODBC.Connection!con.TableExists("GCG_5849_TREE_SHAKER",V.Local.bTableExists3)
		
		F.Intrinsic.Control.If(V.Local.bTableExists1)
			F.Intrinsic.Control.If(V.Local.bTableExists2)
				F.Intrinsic.Control.If(V.Local.bTableExists3)
					'All tables exist...  exit.
					F.Intrinsic.UI.CloseWaitDialog
					F.Intrinsic.Control.ExitSub
				F.Intrinsic.Control.EndIf
			F.Intrinsic.Control.EndIf
		F.Intrinsic.Control.EndIf
		
		F.Intrinsic.UI.ChangeWaitStatus("Setting up the database...")
		
		F.Intrinsic.Control.If(V.Local.bTableExists1,=,False)
			F.ODBC.Connection!con.Execute("CREATE TABLE GCG_5849_TREE_SHAKER ( MACHINEID NUMERIC(6,0) NOT NULL, CUSTOMER CHAR(6) NOT NULL, CUSTOMER_NAME CHAR(30) NOT NULL, GROWER CHAR(30) NOT NULL, STREET CHAR(30) NOT NULL, CITY CHAR(30) NOT NULL, STATE CHAR(20) NOT NULL, ZIP_CODE CHAR(5) NOT NULL, PHONE CHAR(10) NOT NULL, CONTRACT_NUMBER NUMERIC(6,0) NOT NULL, TAX_CODE NUMERIC(4,0) NOT NULL, NUT_TYPE CHAR(30) NOT NULL, MACHINE_TYPE CHAR(30) NOT NULL, SERVICED DATE NOT NULL, BRAND CHAR(30) NOT NULL, TACH_HOURS NUMERIC(8,0) NOT NULL, MACHINE_NUMBER CHAR(30) NOT NULL, SERVICE_TYPE CHAR(30) NOT NULL, ENGINE_TYPE CHAR(30) NOT NULL, MACH_USE CHAR(30) NOT NULL, ENGINE_NUMBER CHAR(30) NOT NULL, CONTACT CHAR(30) NOT NULL, CONTACT_STREET CHAR(30) NOT NULL, CONTACT_CITY CHAR(30) NOT NULL, CONTACT_STATE CHAR(20) NOT NULL, CONTACT_ZIP_CODE CHAR(5) NOT NULL, CONTACT_PHONE_NUMBER CHAR(10) NOT NULL, AIR_CAB BIT DEFAULT '0' NOT NULL, ROTARY_SWEEPS BIT DEFAULT '0' NOT NULL, HEAD_STYLE CHAR(30) NOT NULL, HYDRAULIC_UPDATES BIT DEFAULT '0' NOT NULL, FWO_FORMAT CHAR(1) NOT NULL, HYDRASHAKE BIT DEFAULT '0' NOT NULL, COMMENTS LONGVARCHAR NOT NULL );")
			F.ODBC.Connection!con.Execute("CREATE UNIQUE NOT MODIFIABLE INDEX GCG_5849_TREE_SHAKER_I00 ON GCG_5849_TREE_SHAKER (MACHINEID);")
		F.Intrinsic.Control.EndIf

		F.Intrinsic.Control.If(V.Local.bTableExists2,=,False)
			F.ODBC.Connection!con.Execute("CREATE TABLE GCG_5849_WIND_MACHINE(MACHINEID NUMERIC(6,0) NOT NULL ,CUSTOMER CHAR(6) NOT NULL ,ORCHARD CHAR(30) NOT NULL ,CUSTOMER_NAME CHAR(30) NOT NULL ,GROWER CHAR(30) NOT NULL ,STREET CHAR(30) NOT NULL ,CITY CHAR(30) NOT NULL ,STATE CHAR(20) NOT NULL ,ZIP_CODE CHAR(5) NOT NULL ,PHONE CHAR(10) NOT NULL ,CONTRACT_NUMBER CHAR(30) NOT NULL ,AREA CHAR(2) NOT NULL ,MACHINE_NUMBER CHAR(30) NOT NULL ,BLADE_NUMBER CHAR(30) NOT NULL ,TOP_BOX_NUMBER CHAR(30) NOT NULL ,BOTTOM_NUMBER CHAR(30) NOT NULL ,TOWER_NUMBER CHAR(30) NOT NULL ,ENGINE_NUMBER CHAR(30) NOT NULL ,FRUIT_TYPE CHAR(30) NOT NULL ,INSTALLED DATE NOT NULL ,SERVICED DATE NOT NULL ,MACHINE_TYPE CHAR(30) NOT NULL ,ENGINE_TYPE CHAR(30) NOT NULL ,FUEL_TYPE CHAR(30) NOT NULL ,BRAND CHAR(30) NOT NULL ,SERVICE_TYPE CHAR(30) NOT NULL ,TAX_CODE NUMERIC(4,0) NOT NULL ,TILT CHAR(30) NOT NULL ,FWO_FORMAT CHAR(1) NOT NULL ,CONTOUR_CUT CHAR(30) NOT NULL ,ROTATION CHAR(2) NOT NULL ,EXT_WARRANTY BIT DEFAULT '0' NOT NULL ,AUTO_START BIT DEFAULT '0' NOT NULL ,CLUTCH CHAR(1) NOT NULL ,LATTITUDE CHAR(30) NOT NULL ,DOG_LEG BIT DEFAULT '0' NOT NULL ,LONGITUDE CHAR(30) NOT NULL ,MODEL CHAR(30) NOT NULL ,OR_SAT_CELL CHAR(1) NOT NULL ,INSTALL DATE NOT NULL );")
			F.ODBC.Connection!con.Execute("CREATE UNIQUE NOT MODIFIABLE INDEX GCG_5849_WIND_MACHINE_I00 ON GCG_5849_WIND_MACHINE(MACHINEID);")
		F.Intrinsic.Control.EndIf
		
		F.Intrinsic.Control.If(V.Local.bTableExists3,=,False)
			F.ODBC.Connection!con.Execute("CREATE TABLE GCG_5849_MACHINE_EXTRA (MACHINEID NUMERIC(6,0) NOT NULL ,ORCHARD_BLOCK_NUMBER CHAR(6) NOT NULL ,CLOSEST_ROAD_ACCESS CHAR(30) NOT NULL ,CLOSEST_AVE_ACCESS CHAR(30) NOT NULL ,WM_MANUFACTURE CHAR(30) NOT NULL ,WM_MODEL CHAR(30) NOT NULL ,TYPE_OF_TOWER CHAR(30) NOT NULL ,TYPE_OF_FAN CHAR(30) NOT NULL ,TYPE_OF_FAN_HUB CHAR(30) NOT NULL ,TYPE_OF_FAN_ROTATION CHAR(2) NOT NULL ,TYPE_OF_TOP_HEAD CHAR(30) NOT NULL ,TYPE_OF_TOWER_ROTATION CHAR(30) NOT NULL ,TYPE_OF_BELL CHAR(30) NOT NULL ,TYPE_OF_LOWER_GEAR_BOX CHAR(30) NOT NULL ,POWER_COMP_METER_NUM CHAR(30) NOT NULL ,POWER_COMP_PLANT_NUM CHAR(30) NOT NULL ,POWER_COMP_NAME CHAR(30) NOT NULL ,ENGINE_USAGE_HOURS NUMERIC(6,0) NOT NULL ,ENGINE_HORSEPOWER NUMERIC(4,0) NOT NULL ,ENGINE_TIMING NUMERIC(2,0) NOT NULL ,ENGINE_VACUUM NUMERIC(2,0) NOT NULL ,ENGINE_DWELL NUMERIC(2,0) NOT NULL ,ENGINE_RPM NUMERIC(4,0) NOT NULL ,CUST_MACH_NUMBER CHAR(30) NOT NULL ,CUST_MACH_LETTER CHAR(30) NOT NULL ,ROTATION_MOTOR_MFG CHAR(30) NOT NULL ,METER_CAN_MFG CHAR(30) NOT NULL ,ELECTRIC_PANEL_MFG CHAR(30) NOT NULL ,HEAD_ROTATION_TIME NUMERIC(4,0) NOT NULL ,TRACKING_GOOD BIT DEFAULT '0' NOT NULL ,PHASE_MOTOR_INSTALLED BIT DEFAULT '0' NOT NULL ,MOTOR_FRAME_NUMBER CHAR(30) NOT NULL ,L1_AMP NUMERIC(3,0) NOT NULL ,L2_AMP NUMERIC(3,0) NOT NULL ,L3_AMP NUMERIC(3,0) NOT NULL ,M1 NUMERIC(3,0) NOT NULL ,M2 NUMERIC(3,0) NOT NULL ,M3 NUMERIC(3,0) NOT NULL ,KEY_NUMBER CHAR(30) NOT NULL ,DRIVING_CONDITIONS CHAR(30) NOT NULL ,CUSTOMER_BLOCK_LETTER CHAR(30) NOT NULL ,ELE_MOTOR_MFG CHAR(30) NOT NULL ,ELE_MOTOR_RPM NUMERIC(4,0) NOT NULL ,COMMENTS LONGVARCHAR NOT NULL );")
			F.ODBC.Connection!con.Execute("CREATE UNIQUE NOT MODIFIABLE INDEX GCG_5849_MACHINE_EXTRA_I00 ON GCG_5849_MACHINE_EXTRA(MACHINEID);")
		F.Intrinsic.Control.EndIf

		F.Intrinsic.UI.CloseWaitDialog
		
	F.Intrinsic.Control.Catch
		F.Intrinsic.Control.CallSub(Catching,"Sub",V.Ambient.CurrentSubroutine,"ErrorNo",V.Ambient.ErrorNumber,"ErrorDesc",V.Ambient.ErrorDescription) 
	F.Intrinsic.Control.EndTry
