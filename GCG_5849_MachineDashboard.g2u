Program.Sub.cmdCustomerLookup_Click.Start
	F.Intrinsic.Control.Try
	V.Local.sError.Declare
	
		'Customer Lookup
		'Where to get TAX CODE from?
		V.Local.sRet.Declare
		V.Local.sSQL.Declare
		V.Local.sCustomer.Declare
		F.Intrinsic.UI.Browser(205,"",V.Local.sRet)
		F.Intrinsic.Control.If(V.Local.sRet,<>,"***CANCEL***")
			F.Intrinsic.String.Split(V.Local.sRet,"*!*",V.Local.sRet)
			F.Intrinsic.String.Build("select CUSTOMER, NAME_CUSTOMER, ADDRESS1, CITY, STATE, ZIP, TELEPHONE from V_CUSTOMER_MASTER where CUSTOMER='{0}'",V.Local.sRet(0),V.Local.sSQL)
			F.ODBC.Connection!con.ExecuteAndReturn(V.Local.sSQL,V.Local.sCustomer)
		F.Intrinsic.Control.EndIf
		F.Intrinsic.String.Split(V.Local.sCustomer,"*!*",V.Local.sCustomer)	
		F.Intrinsic.Control.SelectCase(V.Args.Screen)
			F.Intrinsic.Control.Case("FRMSHAKER")
				Gui.frmShaker.txtCustomer.Text(V.Local.sCustomer(0))
				Gui.frmShaker.txtCustomerName.Text(V.Local.sCustomer(1).Trim)
				Gui.frmShaker.txtGrower.Text(V.Local.sCustomer(1).Trim)
				Gui.frmShaker.txtStreetAddress.Text(V.Local.sCustomer(2).Trim)
				Gui.frmShaker.txtCity.Text(V.Local.sCustomer(3).Trim)
				Gui.frmShaker.ddlState.Text(V.Local.sCustomer(4).Trim)
				Gui.frmShaker.txtZipCode.Text(V.Local.sCustomer(5).Trim)
				Gui.frmShaker.txtPhone.Text(V.Local.sCustomer(6).Trim)
			F.Intrinsic.Control.Case("FRMWINDMACHINE")
				Gui.frmWindMachine.txtCustomer.Text(V.Local.sCustomer(0))
				Gui.frmWindMachine.txtCustomerName.Text(V.Local.sCustomer(1).Trim)
				Gui.frmWindMachine.txtGrower.Text(V.Local.sCustomer(1).Trim)
				Gui.frmWindMachine.txtOrchard.Text(V.Local.sCustomer(1).Trim)
				Gui.frmWindMachine.txtStreetAddress.Text(V.Local.sCustomer(2).Trim)
				Gui.frmWindMachine.txtCity.Text(V.Local.sCustomer(3).Trim)
				Gui.frmWindMachine.ddlState.Text(V.Local.sCustomer(4).Trim)
				Gui.frmWindMachine.txtZipCode.Text(V.Local.sCustomer(5).Trim)
				Gui.frmWindMachine.txtPhone.Text(V.Local.sCustomer(6).Trim)
		F.Intrinsic.Control.EndSelect
		
	F.Intrinsic.Control.Catch
		F.Intrinsic.Control.CallSub(Catching,"Sub",V.Ambient.CurrentSubroutine,"ErrorNo",V.Ambient.ErrorNumber,"ErrorDesc",V.Ambient.ErrorDescription) 
	F.Intrinsic.Control.EndTry
